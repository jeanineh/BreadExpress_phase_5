  <table class="table table-striped" id="item_price_history">
    <thead>
      <tr>
        <th width="225">Price</th>
        <th width="125">Start Date</th>
        <th width="200">End Date</th>
        <th width="200"></th>
      </tr>
    </thead>
    <tbody>
      <% @item_prices.each do |item_price| %>
        <tr>
          <td><%= number_to_currency item_price.price %></td>
          <td><%= item_price.start_date.strftime('%m-%d-%y') %></td>
          <% unless item_price.end_date.nil? %>
            <td><%= item_price.end_date.strftime('%m-%d-%y') %></td>
          <% end %>
          <td></td>
        </tr>
      <% end %>
    </tbody>
  </table> 

<p>&nbsp;</p>